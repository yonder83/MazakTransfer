<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.data xdt:Transform="Insert">
    <DbProviderFactories>
      <remove invariant="System.Data.SqlServerCe.4.0"/>
      <add name="Microsoft SQL Server Compact Data Provider 4.0" invariant="System.Data.SqlServerCe.4.0" description=".NET Framework Data Provider for Microsoft SQL Server Compact" type="System.Data.SqlServerCe.SqlCeProviderFactory, System.Data.SqlServerCe, Version=4.0.0.1, Culture=neutral, PublicKeyToken=89845dcd8080cc91"/>
    </DbProviderFactories>
  </system.data>
  <applicationSettings>
    <MazakTransfer.Properties.Settings>
      <setting name="LocalPath" serializeAs="String" xdt:Locator="Match(name)" xdt:Transform="Replace">
        <value>C:\Mazak_ATA</value>
      </setting>
      <setting name="MazakPath" serializeAs="String" xdt:Locator="Match(name)" xdt:Transform="Replace">
        <value>\\Mazak\OHJELMAT</value>
      </setting>
      <setting name="DatabasePath" serializeAs="String" xdt:Locator="Match(name)" xdt:Transform="Replace">
        <value>C:\temp\MazakTransfer\Database</value>
      </setting>
    </MazakTransfer.Properties.Settings>
  </applicationSettings>
</configuration>